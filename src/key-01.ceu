#include "c.ceu"
#include "media/media.ceu"

var int width = 1080;
var int height = 720;

var&? Scene s = spawn Scene(Size(width,height),true);
watching s do
    var _char_ptr_ext key;
    var bool pressed;
    every (key,pressed) in s.scene.ok_keyed do
        _printf("KEY: %s %d\n", key, pressed as int);
    end
end

escape 0;
