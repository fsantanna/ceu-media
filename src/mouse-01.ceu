#include "c.ceu"
#include "media/media.ceu"

var int width = 1080;
var int height = 720;

var& IScene scene;
watching Scene(Size(width,height),true) => (scene) do
    var f64 x, y;
    var int but, press;
    every (x,y,but,press) in scene.ok_mouse_clicked do
        _printf("ok_mouse_clicked: %f %f %d %d\n", x,y,but,press);
    end
end

escape 0;
